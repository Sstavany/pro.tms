{
  "version": 2,
  "builds": [
    {
      "src": "project/api/api_handler.py",
      "use": "@vercel/python"
    },
    {
      "src": "project/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "project/api/api_handler.py"
    },
    {
      "src": "/(.*)",
      "dest": "project/public/$1"
    }
  ]
}